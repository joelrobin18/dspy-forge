# Application Settings
LOG_LEVEL=INFO
LOG_FILE=./logs/app.log

# Storage Backend Configuration
# Options: "local" or "databricks"
STORAGE_BACKEND=local

# Local Storage Settings (when STORAGE_BACKEND=local)
LOCAL_STORAGE_PATH=./artifacts/workflows

# Databricks Storage Settings (when STORAGE_BACKEND=databricks)
DATABRICKS_VOLUME_PATH=/Volumes/catalog/schema/volume/workflows
DATABRICKS_HOST=https://your-workspace.cloud.databricks.com
DATABRICKS_TOKEN=your-access-token

# CORS Settings
ALLOWED_ORIGINS=["http://localhost:3000", "http://127.0.0.1:3000"]