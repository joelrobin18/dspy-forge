# Application Settings
LOG_LEVEL=INFO

# Storage Backend Configuration
STORAGE_BACKEND=local # Options: "local" or "databricks"
ARTIFACTS_PATH=./artifacts/

# Databricks Configuration
# Configure PAT or OAuth authentication via Databricks CLI
DATABRICKS_CONFIG_PROFILE=profile-name
DATABRICKS_HOST=https://your-workspace.cloud.databricks.com
DATABRICKS_TOKEN=your-access-token
DATABRICKS_WAREHOUSE_ID=your-warehouse-id  # Required for optimization - SQL warehouse ID

# LM Provider API Keys (for non-Databricks models)
# OpenAI
OPENAI_API_KEY=sk-...

# Anthropic
ANTHROPIC_API_KEY=sk-ant-...

# Google Gemini
GEMINI_API_KEY=...

# Custom LM Provider (for any other provider)
CUSTOM_LM_API_BASE=https://api.example.com/v1
CUSTOM_LM_API_KEY=...

# CORS Settings
ALLOWED_ORIGINS=["http://localhost:3000", "http://127.0.0.1:3000"]